{
  "name": "yuncms/yuncms-user",
  "description": "The user module for yuncms.",
  "keywords": [
    "yii2",
    "yii2-user",
    "user management"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "xutl/yii2-wechat": "~2.0.0"
  },
  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\user\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "id": "user",
      "translate": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/user/messages"
      }
    },
    "bootstrap": "yuncms\\user\\Bootstrap",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
